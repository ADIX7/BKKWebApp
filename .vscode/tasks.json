{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/BKKWebApp/BKKWebApp.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Generate SignalRHandlers",
            "command": "dotnet-script",
            "type": "process",
            "args": [
                "${workspaceFolder}/src/BKKWebApp/websrc/SignalRHandlers.csx",
                "--no-cache"
            ],
            "problemMatcher": []
        }
    ]
}